{
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "contractor.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/contractors", "dest": "/contractor.html" },
    { "src": "/contractor", "dest": "/contractor.html" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
